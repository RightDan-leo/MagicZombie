VITE_FIREBASE_API_KEY=your-api-key
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
# Optional: set a default profile id for local dev/testing
# VITE_DEFAULT_PROFILE_ID=dev-player
# Optional: testing flag to mirror all weapon levels
# VITE_ENABLE_WEAPON_LEVEL_SYNC=true

# Backend telemetry server configuration
SERVER_PORT=5050
SESSION_SECRET=change-me
ADMIN_USERNAME=admin
ADMIN_PASSWORD=change-me
TELEMETRY_INGEST_TOKEN=super-secret-token
# Optional custom database path
# TELEMETRY_DB_PATH=./data/telemetry.db

# Frontend telemetry reporting (must match backend ingest settings)
VITE_TELEMETRY_API_URL=http://localhost:5050/api/telemetry
VITE_TELEMETRY_TOKEN=super-secret-token
